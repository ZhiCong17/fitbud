<%= simple_form_for [exercise_plan, tracker], html: { class: "d-flex align-items-center gap-2" } do |f| %>
  <%= f.input :weight, label: false, input_html: { class:"text-center", placeholder: "KG"} %>
  <%= f.input :repetitions, label: false, input_html: { class: "text-center d-inline", placeholder: "Reps"} %>
  <%= f.submit "+", class: "btn btn-primary mb-3", html: {data: {disable_button_target: "submit"}} %>
<% end %>
